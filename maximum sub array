 int osm=arr[0];
        int csum=arr[0];
        for (int i=0;i<arr.length;i++){
            if (csum>=0){
                csum+=arr[i];
            }
            else {
                csum=arr[i];
            }
            if (osm<csum){
                osm =csum;
            }
        }
        return osm;
